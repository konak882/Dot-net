
@{
    ViewBag.Title = "Order";
    Layout = "~/Views/Shared/_innerview_res.cshtml";
}

<h2>Order</h2>

<table class="table table-bordered">
<tr>
    <th>Name</th>
    <th>Production Date</th>
    <th>Expired Date</th>
    <th>Status</th>
    <th></th>

</tr>
@foreach (var c in Model)
{
    <tr>

        <td>@c.order_name</td>
        <td>@c.production</td>
        <td>@c.expire</td>
        <td>@c.status</td>
        <td><a href="/Res/Edit/@c.order_id">Edit</a></td>
        
    </tr>

}



</table>